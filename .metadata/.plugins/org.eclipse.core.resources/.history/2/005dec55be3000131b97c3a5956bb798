package test.parser;

import java.util.ArrayList;
import java.util.List;

import test.ip.IPv4;
import test.ip.IPv6;

public abstract class Parser {
	private List<IPv4> listIPv4 = new ArrayList<>();
	private List<IPv6> listIPv6 = new ArrayList<>();

	public Parser() {
	}

	public Parser(String str) {
	}

	/**
	 * @return the listIPv4
	 */
	public List<IPv4> getListIPv4() {
		return listIPv4;
	}

	/**
	 * @return the listIPv6
	 */
	public List<IPv6> getListIPv6() {
		return listIPv6;
	}

	abstract public void parse(String str);

}
