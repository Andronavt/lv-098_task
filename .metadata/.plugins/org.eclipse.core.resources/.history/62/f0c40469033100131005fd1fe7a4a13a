package test;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import java.util.Date;
import java.util.Scanner;

import test.parser.Parser1;

public class Test2 {
	public static void main(String[] args) {
		Date start = null;
		Date end = null;
		File f = new File("C:\\Users\\orecto\\Desktop\\res1.txt");
		Scanner scanner = null;
		FileReader fReader = null;
		BufferedReader bReader = null;
		Parser1 p1 = null;
		Parser1 p2 = null;
		Parser1 p3 = null;
		try {
			scanner = new Scanner(f);
			start = new Date();
			p1 = new Parser1(scanner);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		end = new Date();
		System.out.println(p1.getListIPv4().size());
		System.out.println(p1.getListIPv6().size());
		System.out.println("duration Scanner "
				+ (end.getTime() - start.getTime()) + " ms");
		scanner.close();

		try {
			fReader = new FileReader(f);
			start = new Date();
			p2 = new Parser1(fReader);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		end = new Date();
		System.out.println(p2.getListIPv4().size());
		System.out.println(p2.getListIPv6().size());
		System.out.println("duration FileReader"
				+ (end.getTime() - start.getTime()) + " ms");
		try {
			fReader.close();
		} catch (IOException e) {
			e.printStackTrace();
		}

		try {
			fReader = new FileReader(f);
			bReader = new BufferedReader(fReader);
			start = new Date();
			p2 = new Parser1(bReader);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		end = new Date();
		System.out.println(p2.getListIPv4().size());
		System.out.println(p2.getListIPv6().size());
		System.out.println("duration BufferedReader(FileReader)"
				+ (end.getTime() - start.getTime()) + " ms");
		try {
			bReader.close();
			fReader.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
