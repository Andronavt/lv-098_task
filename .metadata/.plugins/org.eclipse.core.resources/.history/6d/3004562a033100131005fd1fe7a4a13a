package test;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import java.io.Reader;
import java.util.Date;
import java.util.Scanner;

import test.parser.Parser1;

public class Test2 {
	public static void main(String[] args) {
		Date start = null;
		Date end = null;
		File f = new File("C:\\Users\\orecto\\Desktop\\res1.txt");
		Scanner scanner = null;
		Reader reader = null;
		Parser1 p1 = null;
		Parser1 p2 = null;
		try {
			scanner = new Scanner(f);
			start = new Date();
			p1 = new Parser1(scanner);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		end = new Date();
		System.out.println(p1.getListIPv4().size());
		System.out.println(p1.getListIPv6().size());
		System.out.println("duration Scanner " + (end.getTime() - start.getTime())
				+ " ms");
		scanner.close();

		try {
			reader = new FileReader(f);
			start = new Date();
			p2 = new Parser1(reader);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		end = new Date();
		System.out.println(p2.getListIPv4().size());
		System.out.println(p2.getListIPv6().size());
		System.out.println("duration FileReader" + (end.getTime() - start.getTime())
				+ " ms");
		try {
			reader.close();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
