	static void meth(IP ip) {
		if (ip == null)
			System.out.println("NULL ;(");
		else {
			if (ip.getClass().isInstance(IPv4.class)) {
				System.out.println("IPv4");
			} else if (ip.getClass().isInstance(IPv6.class)) {
				System.out.println("IPv6");
			} else {
				System.out.println("ELSE ;(");
			}
		}
	}
